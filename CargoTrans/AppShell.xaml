<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CargoTrans.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CargoTrans.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="CargoTrans">


    <ShellContent
    Title="Home"
    ContentTemplate="{DataTemplate local:LoginView}"
    Route="LoginView" />
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
    Title="Home"
    ContentTemplate="{DataTemplate local:NewDispatchView}"
    Route="NewDispatchView" />

</Shell>
